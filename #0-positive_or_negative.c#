#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main - Prints a random numberand states whether
 *        it is positive,negative or zero
 *
 *Return: Always 0
 */
int main(void)
{
int main(void)

  srand(time(0))
  n=rand() -RAND_MAX/2;

  if (n>1)
    print f ("%d is positive\n",n);
  else if (n<0)
    print f ("% is negative\n",n);
  else
    
    print f ("% is zero\",n);

return(0);
}
